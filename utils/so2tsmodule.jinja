
export var info = 
{
    machine_type        : "{{ info['machine_type'] }}",

    name                : "{{ info['name'] }}",

    load_size           : {{ info['load_size'] }},

    loads               : [
        {% for l in info['loads'] %}
            {{ l   }} , 
        {% endfor %}
    ], 

    {% if 'exported_symbols' in info %}
        exported_symbols    : [
            {% for e in info['exported_symbols'] %}
                {{ e   }} , 
            {% endfor %}
        ],
    {% endif %}

    {% if 'relocations' in info %}
        relocations         : [
            {% for r in info['relocations'] %}
                {{ r   }} , 
            {% endfor %}
        ],
    {% endif %}

    ctors_offset        : {{ info['ctors_offset'] }}, 

    ctor_functions               : [
        {% for t in info['ctors'      ] %}
            {{ t   }} , 
        {% endfor %}
    ],

    dtors_offset        : {{ info['dtors_offset'] }}, 

    dtor_functions               : [
        {% for t in info['dtors'      ] %}
            {{ t   }} , 
        {% endfor %}
    ],


}
;
